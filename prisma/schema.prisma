}

model statuses {
  id           Int            @id @default(autoincrement())
  name         String?
  description  String?
  created_at   DateTime?
  updated_at   DateTime?
  applications applications[]
}

model youth_groups {
  id                        Int                         @id @default(autoincrement())
  name                      String?
  description               String?
  created_at                DateTime?
  updated_at                DateTime?
  applications_youth_groups applications_youth_groups[]
}
